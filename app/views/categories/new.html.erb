<% provide(:title, 'Categoty') %>
<h1><%= t :add_category %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category  do |f| %>

    <%= f.label :'Category Name' %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.submit t :add_text, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>